#include <iostream>
using namespace std ; 
int main() {
    int  N  ; 
    cout << "Enter the number of N :" << endl ;
    cin >> N ; 
    int count = N*N ;  
    for(int i = 1 ; i <= N ; i++){
        for(int j = 1 ; j<=  N ; j++){
            cout << count << "   " ; 
            count-- ; 
        }
        cout << endl ; 
    }
} 
